{
  "name": "groq-desktop-app",
  "version": "1.0.0",
  "description": "A simple Electron app with Groq API integration",
  "main": "electron/main.js",
  "type": "commonjs",
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "dev": "concurrently \"pnpm dev:vite\" \"pnpm dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "vite build",
    "build:electron": "electron-builder",
    "dist": "pnpm build && pnpm build:electron",
    "dist:mac": "pnpm build && electron-builder --mac",
    "dist:win": "pnpm build && electron-builder --win",
    "dist:linux": "pnpm build && electron-builder --linux",
    "test": "node scripts/validate-test-imports.js && jest",
    "test:coverage": "node scripts/validate-test-imports.js && jest --coverage",
    "test:unit": "node scripts/validate-test-imports.js && jest --testPathPattern=__tests__/unit",
    "test:integration": "node scripts/validate-test-imports.js && jest --testPathPattern=__tests__/integration",
    "test:watch": "jest --watch",
    "test:ci": "node scripts/validate-test-imports.js && jest --coverage --ci --watchAll=false --passWithNoTests --maxWorkers=2 --forceExit",
    "test:pipeline": "pnpm run lint && pnpm run test:ci && node scripts/checkCoverage.js",
    "test:validate-imports": "node scripts/validate-test-imports.js",
    "test:cache-perf": "./test-cache-performance.sh",
    "test:clear-cache": "rm -rf .jest-cache",
    "test:platforms": "./test-cross-platform.sh",
    "test:paths": "node __tests__/utils/test-paths.js",
    "test:settings": "node __tests__/utils/test-settingsManager.js",
    "test:shared-dirs": "node __tests__/utils/test-shared-directories.js",
    "benchmark": "node __tests__/benchmarks/benchmark-tests.js",
    "benchmark:jest": "node __tests__/benchmarks/benchmark-jest.js",
    "lint": "npx eslint . --max-warnings 0",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md,css}\""
  },
  "keywords": [
    "electron",
    "groq",
    "desktop",
    "chat"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "ansi-to-html": "^0.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "electron-json-storage": "^4.6.0",
    "groq-sdk": "0.16.0",
    "katex": "^0.16.22",
    "lucide-react": "^0.523.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.3.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-textarea-autosize": "^8.5.9",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.25.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^30.0.5",
    "concurrently": "9.1.2",
    "cross-env": "^7.0.3",
    "electron": "37.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "nock": "^14.0.8",
    "postcss": "^8.4.31",
    "prettier": "^3.6.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.2.5",
    "vite": "^6.2.6"
  },
  "build": {
    "appId": "com.groq.desktop",
    "productName": "Groq Desktop",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "shared/**/*",
      "node_modules/**/*"
    ],
    "asarUnpack": [
      "electron/scripts/*.sh",
      "electron/scripts/*.cmd",
      "electron/scripts/*.ps1"
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "public/icon.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Development",
      "icon": "public/icon.png"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
